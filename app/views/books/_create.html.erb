<div>
  <%= image_tag user.get_profile_image(100,100) %>
  <h3><%= user.name %></h3>
</div>

<h2>New book</h2>

<%= form_with model: book do |f| %>

  <label>Title</label>
    <%= f.text_field :title %>
  
  <label>Opinion</label>
    <%= f.text_area :body %>
  
  <%= f.submit "Create Book" %>

<% end %>