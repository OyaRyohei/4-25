<%= render "books/create", book:@book, user:current_user %>

<h2>Users</h2>

  <tabel>
    <thead>
      <th>image</th>
      <th>name</th>
      <th></th>
    </thead>
    
    <% @users.each do |user| %>
      <tbody>
        <tr>
          <td><%= image_tag user.get_profile_image(100,100) %></td>
          <td><%= user.name %></td>
          <td><%= link_to "Show", user_session_path %></td>
        </tr>
      </tbody>
    <% end %>
  </tabel>